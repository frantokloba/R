# Create a vector 5 size
vec <- c(1, 2, 3, 4, 5)
print(paste("Vector:", toString(vec)))

# Calculate the sum and mean of the vector
vec_sum <- sum(vec)
vec_mean <- mean(vec)
print(paste("Sum of vector:", vec_sum))
print(paste("Mean of vector:", vec_mean))

# Create a matrix
mat <- matrix(c(1, 2, 3, 4, 5, 6), nrow=2, ncol=3)
print("Matrix:")
print(mat)

# Create a matrix
mat <- matrix(c(1, 2, 3, 4, 5, 6), nrow=2, ncol=3)
print("Matrix:")
print(mat)

# Create a matrix2
mat2 <- matrix(c(1, 2, 3, 4, 5, 6), nrow=2, ncol=3)
print("Matrix:")
print(mat)
# Transpose the matrix
mat_transpose <- t(mat)
print("Transposed Matrix:")
print(mat_transpose)

# Multiply the matrix by a scalar
mat_multiplied <- mat * 2
print("Matrix multiplied by 2:")
print(mat_multiplied)

# Calculate the determinant of a square matrix
square_mat <- matrix(c(1, 2, 3, 4), nrow=2)
det_value <- det(square_mat)
print(paste("Determinant of square matrix:", det_value))

/** nice **/

# Create a vector 5 size
vec <- c(1, 2, 3, 4, 5)
print(paste("Vector:", toString(vec)))

# Calculate the sum and mean of the vector
vec_sum <- sum(vec)
vec_mean <- mean(vec)
print(paste("Sum of vector:", vec_sum))
print(paste("Mean of vector:", vec_mean))

# Create a matrix
mat <- matrix(c(1, 2, 3, 4, 5, 6), nrow=2, ncol=3)
print("Matrix:")
print(mat)

# Transpose the matrix
mat_transpose <- t(mat)
print("Transposed Matrix:")
print(mat_transpose)

# Multiply the matrix by a scalar
mat_multiplied <- mat * 2
print("Matrix multiplied by 2:")
print(mat_multiplied)

# Calculate the determinant of a square matrix
square_mat <- matrix(c(1, 2, 3, 4), nrow=2)
det_value <- det(square_mat)
print(paste("Determinant of square matrix:", det_value))

